@article{lian_design_2013,
	title = {On the design of partial encryption scheme for multimedia content},
	volume = {57},
	issn = {08957177},
	url = {http://linkinghub.elsevier.com/retrieve/pii/S0895717711003487},
	doi = {10.1016/j.mcm.2011.06.007},
	abstract = {Partial encryption, also named selective encryption, is regarded as a suitable choice for multimedia content encryption because of the properties of time efficiency, format
compliance and network compliance. To push it to practical applications, its security should be investigated. However, few works have been done to analyze partial encryption
schemes’ security. In this paper, with respect to the existing attacks (cryptographic attack, replacement attack and statistical model based attack), a partial encryption
model is presented, together with some secure encryption principles. Furthermore, based on the principles, a partial encryption scheme in wavelet domain is proposed, and its performances are evaluated, which proves the principles suitable for designing or evaluating multimedia encryption schemes.},
	language = {en},
	number = {11-12},
	urldate = {2018-06-05},
	journal = {Mathematical and Computer Modelling},
	author = {Lian, Shiguo and Chen, Xi},
	month = jun,
	year = {2013},
	keywords = {multimedia encryption, partial encryption, security analysis, DRM},
	pages = {2613--2624}
}

